{
  "name": "marketing-sein",
  "description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
  "version": "0.0.0",
  "cloudflare": {
    "label": "Vite React Template",
    "products": [
      "Workers"
    ],
    "categories": [],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public",
    "publish": true
  },
  "dependencies": {
    "acme-client": "^5.4.0",
    "bcryptjs": "^3.0.2",
    "jose": "^6.0.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.2.4",
    "@eslint/js": "9.25.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "22.15.19",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.27.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "15.15.0",
    "happy-dom": "^18.0.1",
    "hono": "^4.8.0",
    "jsdom": "^26.1.0",
    "miniflare": "^0.0.0-f7c347a67",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "6.3.5",
    "vitest": "^3.2.4",
    "wrangler": "4.16.1"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy --minify src/worker/index.ts --name certificate-manager-worker",
    "dev": "wrangler dev src/worker/index.ts --local --persist --name certificate-manager-worker",
    "lint": "eslint .",
    "db:migrate": "wrangler d1 migrations apply certificate-manager-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply certificate-manager-db",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "test:worker": "vitest run src/worker/**/*.test.ts",
    "test:ui": "vitest run src/react-app/**/*.test.tsx",
    "coverage": "vitest run --coverage"
  },
  "type": "module"
}
